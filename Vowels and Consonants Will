string = input("Enter string:")
vowels = 0
for i in string:
      if(i == 'a' or i == 'e' or i == 'i' or i == 'o' or i == 'u' or i == 'A' or i=='E' or i=='I' or i=='O' or i=='U'):
            vowels=vowels+1
string = string.replace(' ','')
string = string.replace('.','')
length = 0
for char in string:
    length += 1
consanants = length - vowels
print("Number of vowels are:")
print(vowels)
print("Number of Consanants are:")
print(consanants)
